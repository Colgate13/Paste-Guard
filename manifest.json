{
  "manifest_version": 3,
  "name": "Paste Guard",
  "version": "1.0",
  "description": "Display a confirmation before pasting content into any website.",
  "permissions": [],
  "host_permissions": ["<all_urls>"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["src/index.js"],
      "run_at": "document_start"
    }
  ]
}
